{
    "version": "2.0.0",
    "configurations": [
        {
            "name": "Run ReductStore",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/debug/reductstore",
            "args": [
                "--features",
                "fs-backend web-console"
            ],
            "environment": [
                {
                    "name": "RS_DATA_PATH",
                    "value": "/tmp/reductstore_data"
                }
            ],
            "cwd": "${workspaceFolder}",
            "preLaunchTask": "cargo build",
            "stopAtEntry": false
        }
    ]
}
